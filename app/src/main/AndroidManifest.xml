<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".PacienteApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoMedicos"
        tools:targetApi="31">


        <activity
            android:name=".MenuInicio"
            android:exported="true">
        </activity>

        <activity
            android:name=".Registrarse"
            android:exported="true">
        </activity>

        <activity
            android:name=".InicioPaciente"
            android:exported="true">
        </activity>

        <activity
            android:name=".OpcionesMenu"
            android:exported="true">
        </activity>

        <activity
            android:name=".ListadoPaciente"
            android:exported="true">
        </activity>

        <activity
            android:name=".EditarPaciente"
            android:exported="true">
        </activity>

        <activity
            android:name=".AgregarMedicina"
            android:exported="true">
        </activity>

        <activity
            android:name=".ListadoReceta"
            android:exported="true">
        </activity>

        <activity
            android:name=".OpcionesMenuRecetas"
            android:exported="true">
        </activity>

        <activity
            android:name=".EditarReceta"
            android:exported="true">
        </activity>

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>